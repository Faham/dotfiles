command-line:
[shell]
copy:                      dd if=input.img of=/dev/output-device
hex view:                  xxd
enable debugging in vtune: echo "0"|sudo tee /proc/sys/kernel/yama/ptrace_scope
organazing windows:        j=0; for i in `wmctrl -l -p | grep N/A | cut -c1-10`; do
                             top="$(( ($j*380)%1140 ))"
                             left="$(( 1920+(640*($j/3))%2000 ))"
                             wmctrl -i -r $i -e 0,$left,$top,640,480;
                             j="$(( $j+1 ))"
                           done;
tcpdump                    sudo tcpdump -s 0 -w output.pcap -i eth0 udp port 6100
replace in stdin           sed -e 's///g'
untar with target dir      tar -xvf tarfile -C target_dir
grep directory for text    grep -Irli "string" /path
find binary files          find -type f -executable
ffmpeg -f concat -i mylist.txt -c copy output
gs -dBATCH -sOutputFile="output.pdf" -dFirstPage=1 -dLastPage=3 -sDEVICE=pdfwrite input.pdf                        export page 1 to 3 of input.pdf to output.pdf
gs -dBATCH -dNOPAUSE -q -sDEVICE=pdfwrite -dPDFSETTINGS=/prepress -sOutputFile=out.pdf in1.pdf in2.pdf            put in1.pdf and in2.pdf together into out.pdf
  ${string:position:length} Extracts substring from $string at $position.
stringZ=abcABC123ABCabc
echo ${stringZ:0}                            # abcABC123ABCabc
echo ${stringZ:1}                            # bcABC123ABCabc
echo ${stringZ:7}                            # 23ABCabc
echo ${stringZ:7:3}                          # 23A
echo ${stringZ: -4}                          # Cabc
  ${string#substring}      Deletes shortest match of $substring from front of $string.
echo ${stringZ#a*C}      # 123ABCabc
  ${string##substring}     Deletes longest match of $substring from front of $string.
echo ${stringZ##a*C}     # abc
~.                       # disconnect unresponsive ssh session
~?                       # display list of escape characters in a ssh session

