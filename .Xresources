!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         _foreground
xscreensaver.Dialog.background:         _black
xscreensaver.Dialog.topShadowColor:     _black
xscreensaver.Dialog.bottomShadowColor:  _black
xscreensaver.Dialog.Button.foreground:  _foreground
xscreensaver.Dialog.Button.background:  _black

!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    _foreground
xscreensaver.Dialog.text.background:    _black
xscreensaver.Dialog.internalBorderWidth:0
xscreensaver.Dialog.borderWidth:        0
xscreensaver.Dialog.shadowThickness:    2

!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  _foreground
xscreensaver.passwd.thermometer.background:  _black
xscreensaver.passwd.thermometer.width:       8

!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y
