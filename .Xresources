xscreensaver.Dialog.unameFont:          -*-helvetica-medium-r-*-*-24-*-*-*-*-*-iso8859-1
xscreensaver.Dialog.buttonFont:         -*-helvetica-medium-r-*-*-24-*-*-*-*-*-iso8859-1
